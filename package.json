{
  "name": "@sunsave/nestjs-sentry",
  "version": "0.0.1",
  "private": true,
  "contributors": [
    {
      "name": "Dexter Hardy",
      "email": "dexter.hardy@ntegral.com",
      "url": "http://www.ntegral.com"
    },
    {
      "name": "Danielle Huntrods",
      "email": "danielle@sunsave.energy",
      "url": "https://www.sunsave.energy"
    }
  ],
  "description": "Provides an injectable sentry.io client to provide enterprise logging nestjs modules",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunsave/nestjs-sentry"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "publish:npm": "npm publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "nestjs",
    "sentry.io"
  ],
  "author": "Dexter Hardy",
  "license": "ISC",
  "peerDependencies": {
    "@nestjs/common": "^9.0.4",
    "@nestjs/core": "^9.0.4",
    "@sentry/hub": "^7.7.0",
    "@sentry/node": "^7.7.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.0.5",
    "@types/jest": "28.1.6",
    "@types/node": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "codecov": "^3.8.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.3.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^5.0.4"
  },
  "optionalDependencies": {
    "@nestjs/graphql": "^10.0.20"
  }
}
